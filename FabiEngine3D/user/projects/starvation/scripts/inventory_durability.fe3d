11 12
META script_type_update

INT index = 0
INT size = 9
LOOP:
    IF index IS size:
        BREAK
    ELSE:
        IF _durabilities[index] LESS _durabilityHit AND _inventory[index] NOT "":
            fe3d:model_set_visible(_inventory[index], <false>)
            EDIT _inventory[index] = ""
            EDIT _durabilities[index] = 0.0
        INCR index 1
