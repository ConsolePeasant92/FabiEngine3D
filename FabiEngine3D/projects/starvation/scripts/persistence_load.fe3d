6 13
META script_type_init

/// Survival time
LIST lines = fe3d:file_read("progress.txt")
STR line = lines[0]
CAST line INT
CAST line DEC
EDIT _elapsedMS = line
CAST line STR

/// Bag
EDIT lines = fe3d:file_read("bag.txt")
INT length = misc:get_list_size("_bag")
INT index = 0
LOOP:
    IF index IS length:
        BREAK
    ELSE:
        EDIT line = lines[index]
        CAST line INT
        EDIT _bag[index] = line
        CAST line STR
        INCR index 1
